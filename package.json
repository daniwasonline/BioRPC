{
  "name": "biorpc",
  "version": "3.1.0",
  "description": "A Rich Presence creator for Discord. Meant to be used with dsc.bio.",
  "main": "bootstrap.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Unicord",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeanedTaco/BioRPC.git"
  },
  "contributors": [
    "SaturdayNightDead"
  ],
  "license": "MIT",
  "dependencies": {
    "discord-rpc": "^3.1.1",
    "dialog": "^0.3.1",
    "js-yaml": "^3.14.0",
    "is-elevated": "^3.0.0",
    "auto-launch": "^5.0.5",
    "top-level-await": "^1.1.0",
    "discord.bio": "^6.1.3",
    "process-exists": "^4.0.0"
  },
  "devDependencies": {
    "electron": "^9.4.0"
  },
  "build": {
    "appId": "com.unicord.biorpc",
    "productName": "BioRPC",
    "asar": true,
    "win": {
      "icon": "icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": true,
      "packElevateHelper": false,
      "include": "build/installer.nsh",
      "deleteAppDataOnUninstall": false,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "license": "license.txt",
      "language": "2057",
      "shortcutName": "BioRPC",
      "menuCategory": "Unicord"
    }
  }
}
